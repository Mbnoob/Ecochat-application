            <form id="registerd-from" action="/signup" method="post" enctype="multipart/form-data">
                <div class="form-group" id="form-group-1">
                  <input type="text" class="form-control" name="first_name" id="first_name" aria-describedby="emailHelp" placeholder="Enter your full name"> 
                  <span class="eye-btm-see" id="first_name-check"></span>
                </div>
               
                <div class="form-group" id="form-group-2">
                  <input type="email" class="form-control" name="Email_id" id="Email_id" aria-describedby="emailHelp" placeholder="Enter a valid Emails"> 
                  <span class="eye-btm-see" id="Email_id-check"></span>
                </div>

                <div class="form-group mb-0" id="form-group-3">
                  <input type="password" class="form-control" name="passwords" id="Passwords-input2" placeholder="Enter a valid password">
                  <span class="eye-btm-see52" id="password-check"></i></span>
                  <span class="eye-btm-see" id="eye-seen1"><i class="bi bi-eye-fill" style="font-size: 18px;"></i></span>
                </div>

                <div class="form-group mb-0" id="form-group-4">
                  <input type="password" class="form-control" name="confirm_password" id="Passwords-input" placeholder="Confirm your password">
                  <span class="eye-btm-see53" id="confirmPassword-check"></span>
                  <span class="eye-btm-see" id="eye-seen2"><i class="bi bi-eye-fill" style="font-size: 18px;"></i></span>
                  <span><p class="position-relative mb-0" id="password-not-match" style="bottom: 10px; font-size: 13px; font-weight: 700; color:#FF0A54; transition: opacity 0.3s ease, transform 0.3s ease;"></p></span>
                </div>

                <div class="form-group" id="character-div" hidden>
                  <input type="text" class="form-control" name="choose_character" id="choose-character" hidden>
                </div>
                
                <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#staticBackdrop" hidden></button>

                <!-- choose-character modal -->
                <div class="modal fade" id="staticBackdrop" data-backdrop="static" data-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
                  <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable">
                    <div class="modal-content" style="background-color: #53C58A;">
                      <div class="modal-header" style="border-bottom: 2px solid #cbd5e175;">
                        <h5 class="modal-title font-weight-bolder" id="staticBackdropLabel">Choose your character</h5>

                <div class="links">
                    <a href="/signin">Have an accounts?</a>
                </div>
    
                <div class="form-submit">
                    <button type="submit" class="signin-btn mt-3 mb-5" id="signin-btn">Sign up</button>
                    <span class="spinner-border spinner-border-sm" role="status" aria-hidden="true" style="position: relative; left: -50%; top: -12px; display: none;"></span>

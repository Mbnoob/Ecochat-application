    <script src="//code.jquery.com/jquery-1.12.4.js"></script>
    <script src="//code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/notyf@3/notyf.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-Fy6S3B9q64WdZWQUiU+q4/2Lc9npb8tCaSX9FK7E8HnRr0Jz8D6OP9dO5Vg3Q9ct" crossorigin="anonymous"></script>
    <script src="/javascripts/registerd.js"></script>

    <script>
      let allInputs = document.querySelectorAll('input');

      $(document).ready(()=>{
        function validateFirstName() {
        const nameField = $('#first_name');
        const name_check = $('#first_name-check');
        const trimName = nameField.val().trim();
        const from_group = $('#form-group-1');

      if (trimName === '') {

        $(nameField).css('border', '3px solid #000');
        $(from_group).removeClass('mb-0');
        $(name_check).html('');
        return false;

      } else if(trimName.length <= 5) {

        $(nameField).css('border', '3px solid #FF0A54');
        $(from_group).addClass('mb-0');
        $(name_check).html('<i class="bi bi-x-circle-fill" style="color: #FF0A54;"></i>');
        return false;

      }else if(trimName.length >= 6 && trimName.length <= 20){
        $(nameField).css('border', '3px solid #28a745');
        $(from_group).addClass('mb-0');
        $(name_check).html('<i class="bi bi-check-circle-fill" style="color: #28a745;"></i>');
        return true;

      }else{

        $(nameField).css('border', '3px solid #FF0A54');
        $(from_group).addClass('mb-0');
        $(name_check).html('<i class="bi bi-x-circle-fill" style="color: #FF0A54;"></i>');
        return false;

      }
    };

    function EmailVaidation() {
      const Email_input = $('#Email_id');
      const email = Email_input.val().trim();
      const Email_group = $('#form-group-2');
      const Email_check = $('#Email_id-check');
      let EmailRegex = /^[\w-\.]+@([\w-]+\.)+[\w-]{2,4}$/;

      if (email === '') {
        $(Email_input).css('border', '3px solid #000');
        $(Email_group).removeClass('mb-0');
        $(Email_check).html('');
        return false;

      } else if(EmailRegex.test(email)) {
        $(Email_input).css('border', '3px solid #28a745');
        $(Email_group).addClass('mb-0');
        $(Email_check).html('<i class="bi bi-check-circle-fill" style="color: #28a745;"></i>');
        return true;
      }else{
        $(Email_input).css('border', '3px solid #FF0A54');
        $(Email_group).addClass('mb-0');
        $(Email_check).html('<i class="bi bi-x-circle-fill" style="color: #FF0A54;"></i>');
        return false;
      }
    };

    function PasswordValidation() {
      const password_input = $('#Passwords-input2');
      const password = password_input.val().trim();
      const password_group = $('#form-group-3');
      const password_check = $('#password-check');

      let passwordRegex = /^(?=.*\d)(?=.*[a-z])(?=.*[A-Z])(?=.*[a-zA-Z]).{5,}$/;

      if (password === '') {
        $(password_input).css('border', '3px solid #000');
        $(password_check).html('');
        return false;
      } else if(passwordRegex.test(password)){
        $(password_input).css('border', '3px solid #28a745');
        $(password_group).addClass('mb-0');
        $(password_check).html('<i class="bi bi-check-circle-fill" style="color: #28a745;"></i>');
        return true;
      }else{
        $(password_input).css('border', '3px solid #FF0A54');
        $(password_group).addClass('mb-0');
        $(password_check).html('<i class="bi bi-x-circle-fill" style="color: #FF0A54;"></i>');
        return false;
      }
    };

    function confirmPasswordValidation() {
      const confirm_input = $('#Passwords-input');
      const main_password = $('#Passwords-input2').val().trim()
      const confirm_passwords = confirm_input.val().trim();
      const confirm_group = $('#form-group-4');
      const confirm_check = $('#confirmPassword-check');
      const confirm_message = $('#password-not-match');

      if (confirm_passwords === '') {
        $(confirm_input).css('border', '3px solid #000');
        $(confirm_check).html('');
        $(confirm_message).text('');
        return false;
      } else if(main_password === ''){
        $(confirm_input).css('border', '3px solid #FF0A54');
        $(confirm_check).html('<i class="bi bi-x-circle-fill" style="color: #FF0A54;"></i>');
        $(confirm_message).text('Upper Password field is empty !');
        return false;
      }else if(main_password !== confirm_passwords){
        $(confirm_input).css('border', '3px solid #FF0A54');
        $(confirm_check).html('<i class="bi bi-x-circle-fill" style="color: #FF0A54;"></i>');
        $(confirm_message).text('Password is not match !');
        return false;
      }else{
        $(confirm_input).css('border', '3px solid #28a745');
        $(confirm_check).html('<i class="bi bi-check-circle-fill" style="color: #28a745;"></i>');
        $(confirm_message).text('');
        return true;
      }
    };

        $('#first_name').on('input', validateFirstName);

        $('#Email_id').on('input', EmailVaidation);

        $('#Passwords-input2').on('input', PasswordValidation);

        $('#Passwords-input').on('input', confirmPasswordValidation);
      });

const notyf = new Notyf({
        position: {
        x: 'center',
        y: 'top',
      },
      types: [
    {
      background: 'linear-gradient(90deg, hsla(350, 93%, 61%, 1) 0%, hsla(8, 98%, 59%, 1) 100%)',
      icon: '<i class="bi bi-x-circle-fill"></i>'
    }
  ]
      });

    $(window).on('load', ()=>{
      let errors = "<%= error_msg %>";
    
        if (errors) {
          notyf.open({
          message: errors,
          duration: 5000
        })
        };
    });

    $(document).ready(()=>{
      let selectedCharacter = '';

      $('#registerd-from').submit(function (event) {

        if ($('#first_name').val() === '' || $('#Email_id').val() === '' || $('#Passwords-input2').val() === '' || $('#Passwords-input').val() === '') {
          event.preventDefault();
          notyf.open({
          message: 'All fields are required !',
          duration: 5000
        });
        $(allInputs).css('border', '3px solid #FF0A54');
        $(allInputs).effect('shake');
        return;
        }
        
        if ($('#choose-character').val() === '') {
          $('#staticBackdrop').modal('show');
          event.preventDefault();
          return;
        }
      });

      $('.all-character').click(function(e){
        if ($('#character-div').find($('#choose-character')).length > 0) {
          $('.all-character').css('border', '3px solid #f8f9fa');
          $('#selsc-file-icon').prop('src', '/src/svg/file.svg');
          $(this).css('border', '3px solid #fec347');
          selectedCharacter = $(this).data('character');
          $('#save-button').prop('disabled', false);
          $('#save-button').css('cursor', 'pointer');
          return;
        }else{
          $('#character-div').html('');
          $('#selsc-file-icon').prop('src', '/src/svg/file.svg');
          let characterInput = $('<input>').attr('type', 'text');
          characterInput.attr('name', 'choose_character');
          characterInput.attr('id', 'choose-character');
          characterInput.prop('hidden', true);
          $('#character-div').append(characterInput);

          $('.all-character').css('border', '3px solid #f8f9fa');
          $(this).css('border', '3px solid #fec347');
          selectedCharacter = $(this).data('character');
          $('#save-button').prop('disabled', false);
          $('#save-button').css('cursor', 'pointer');
          return;
        }
      });

      $('#button-file').click(function (e) {
        e.preventDefault();
        $('#character-div').html('');
        $('.all-character').css('border', '3px solid #f8f9fa');
        $('#save-button').prop('disabled', true);
        $('#save-button').css('cursor', 'no-drop');
        let inputElement = $('<input>').attr('type', 'file')
        inputElement.attr('name', 'file');
        inputElement.attr('id', 'choose_pic');
        inputElement.addClass('form-control');
        inputElement.prop('hidden', true);
        $('#character-div').append(inputElement)
        $(inputElement).click();

        $(inputElement).on('change', (e)=>{
          let outputIMG = $('#selsc-file-icon');
          let files = e.target.files[0];
          let validExtensions = ['image/png', 'image/jpeg', 'image/jpg'];

          if (files && validExtensions.indexOf(files.type) === -1) {
            $('#registerd-from').trigger("reset");
            notyf.error({
              message: 'Only .png, .jpeg, .jpg format are accepted...',
              duration: 5000
            })
            return;
            
          }else if(files && files.size <= 5242880){
            outputIMG.prop('hidden', true);
            $('#image-preloader').removeClass('d-none');
            let reader = new FileReader();
            reader.readAsDataURL(files);
            reader.onload = (e)=>{
              let result = e.target.result;
              outputIMG.prop('src', result);
              $('#image-preloader').addClass('d-none');
              outputIMG.prop('hidden', false);
              $('#save-button').prop('disabled', false);
              $('#save-button').css('cursor', 'pointer');
            }
          }else{
            $('#registerd-from').trigger("reset");
            notyf.error({
              message: 'File size should be less then or equal 5MB...',
              duration: 5000
            })
            return;
          }
        })
      });

      //reset the modal
      $('#close_modal').click((e)=>{
        e.preventDefault();
          $('#choose-character').val('');
          $('#character-div').html('');
          $('#selsc-file-icon').prop('src', '/src/svg/file.svg');
          $('#save-button').prop('disabled', true);
          $('#save-button').css('cursor', 'no-drop');
          $('.all-character').css('border', '3px solid #f8f9fa');

          let characterInput = $('<input>').attr('type', 'text');
          characterInput.attr('name', 'choose_character');
          characterInput.attr('id', 'choose-character');
          characterInput.prop('hidden', true);
          $('#character-div').append(characterInput);
          return;
      })

      $('#skipe-button').click(function () {
          $('#choose-character').val('src/user1.jpg');
           $('#staticBackdrop').modal('hide');
           $('#registerd-from').submit();
           return;
      });

      $('#save-button').click(function () {
          $('#choose-character').val(selectedCharacter);
           $('#staticBackdrop').modal('hide');
           $('#registerd-from').submit();
           return;
      });
    });
    
    </script>
</body>

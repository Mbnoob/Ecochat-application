    <script src="/javascripts/index.js"></script>

    <script>
     $(window).on('load', ()=>{
      let errors = "<%= error_msg %>";
      const notyf = new Notyf({
        position: {
        x: 'center',
        y: 'top',
      },
      dismissible: true,
      types: [
    {
      background: 'linear-gradient(90deg, hsla(350, 93%, 61%, 1) 0%, hsla(8, 98%, 59%, 1) 100%)',
      icon: '<i class="bi bi-x-circle-fill"></i>'
    }
  ]
      });
        if (errors) {
          notyf.open({
          message: errors,
          duration: 5000
        })
        }
    });
    </script>

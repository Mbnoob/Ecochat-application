        <form id="signin-form" method="post" action="/signin">
            <div class="form-group">
              <input type="email" class="form-control" name="emails" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter a valid Emails"> 
            </div>
            <div class="form-group" style="margin-bottom: 0; line-height: 0;">
              <input type="password" class="form-control" name="password" id="exampleInputPassword1" placeholder="Enter a valid password">
              <span class="eye-btm-see" id="eye-seen" style="cursor: pointer;"><i class="bi bi-eye-fill" style="font-size: 18px; "></i></span>
            </div>

            <div class="links">
                <a href="#">Forget Passwords?</a>
                <a href="/signup">Create an accounts?</a>
            </div>

            <div class="form-submit">
                <button type="submit" class="signin-btn" id="signin-btn">Sign in</button>
                <span class="spinner-border spinner-border-sm" role="status" aria-hidden="true" style="position: relative; left: -50%; top: -12px; display: none;"></span>
            </div>
           
          </form>
